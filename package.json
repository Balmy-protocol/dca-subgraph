{
  "name": "dca-subgraph",
  "version": "1.0.0",
  "license": "",
  "scripts": {
    "codegen": "rm -rf generated && graph codegen",
    "build:rinkeby": "yarn prepare:rinkeby && graph build",
    "build:ropsten": "yarn prepare:ropsten && graph build",
    "build:kovan": "yarn prepare:kovan && graph build",
    "build:goerli": "yarn prepare:goerli && graph build",
    "build:mainnet": "yarn prepare:mainnet && graph build",
    "thegraph:deploy:rinkeby": "graph deploy --debug --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ alejoamiras/dca-rinkeby",
    "thegraph:deploy:rinkeby-stable": "graph deploy --debug --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ mean-finance/dca-v1-rinkeby",
    "thegraph:deploy:ropsten": "graph deploy --debug --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ alejoamiras/dca-ropsten",
    "thegraph:deploy:ropsten-stable": "graph deploy --debug --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ mean-finance/dca-v1-ropsten",
    "thegraph:deploy:kovan": "graph deploy --debug --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ alejoamiras/dca-kovan",
    "thegraph:deploy:kovan-stable": "graph deploy --debug --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ mean-finance/dca-v1-kovan",
    "thegraph:deploy:goerli": "graph deploy --debug --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ alejoamiras/dca-goerli",
    "thegraph:deploy:goerli-stable": "graph deploy --debug --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ mean-finance/dca-v1-goerli",
    "thegraph:deploy:mainnet": "graph deploy --debug --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ mean-finance/dca",
    "thegraph:deploy:mainnet-stable": "graph deploy --debug --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ mean-finance/dca-v1",
    "prepare:rinkeby": "mustache config/rinkeby.json subgraph.template.yaml > subgraph.yaml",
    "prepare:ropsten": "mustache config/ropsten.json subgraph.template.yaml > subgraph.yaml",
    "prepare:kovan": "mustache config/kovan.json subgraph.template.yaml > subgraph.yaml",
    "prepare:goerli": "mustache config/goerli.json subgraph.template.yaml > subgraph.yaml",
    "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml",
    "deploy:rinkeby": "yarn build:rinkeby && yarn thegraph:deploy:rinkeby",
    "deploy:rinkeby:stable": "yarn build:rinkeby && yarn thegraph:deploy:rinkeby-stable",
    "deploy:ropsten": "yarn build:ropsten && yarn thegraph:deploy:ropsten",
    "deploy:ropsten:stable": "yarn build:ropsten && yarn thegraph:deploy:ropsten-stable",
    "deploy:kovan": "yarn build:kovan && yarn thegraph:deploy:kovan",
    "deploy:kovan:stable": "yarn build:kovan && yarn thegraph:deploy:kovan-stable",
    "deploy:goerli": "yarn build:goerli && yarn thegraph:deploy:goerli",
    "deploy:goerli:stable": "yarn build:goerli && yarn thegraph:deploy:goerli-stable",
    "release": "standard-version",
    "deploy:mainnet": "yarn build:mainnet && yarn thegraph:deploy:mainnet",
    "deploy:mainnet:stable": "yarn build:mainnet && yarn thegraph:deploy:mainnet-stable",
    "lint:check": "prettier --check './**'",
    "lint:fix": "sort-package-json && prettier --write './**'",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.21.1",
    "@graphprotocol/graph-ts": "0.20.1",
    "mustache": "4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "husky": "7.0.1",
    "pinst": "2.1.6",
    "prettier": "2.3.2",
    "sort-package-json": "1.50.0",
    "standard-version": "9.3.1"
  },
  "authors": [
    {
      "name": "Alejo Amiras",
      "email": "alejo.amiras@gmail.com",
      "url": "https://github.com/alejoamiras"
    }
  ]
}
